{
	"name": "grow-run-archive",
	"version": "0.0.1",
	"private": true,
	"packageManager": "npm@10.8.0",
	"scripts": {
		"build": "dotenv --env-file=./.env.test turbo run build",
		"dev": "dotenv --env-file=./.env.test turbo run dev",
		"start": "dotenv --env-file=./.env.test turbo run start",
		"lint": "turbo run lint",
		"e2e-pipeline": "cd e2e-tests && npm run pipeline"
	},
	"devDependencies": {
		"dotenv-cli": "^8.0.0",
		"tslib": "^2.8.1",
		"turbo": "^2.5.3",
		"typescript": "^5.8.3"
	},
	"type": "module",
	"workspaces": [
		"app",
		"backend",
		"e2e-tests",
		"packages/*"
	]
}