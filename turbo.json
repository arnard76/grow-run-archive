{
    "$schema": "https://turbo.build/schema.json",
    "tasks": {
        "build": {
            "inputs": [
                "$TURBO_DEFAULT$",
                "app/.env"
            ],
            "outputs": [
                "dist/**",
                ".svelte-kit/**"
            ]
        },
        "dev": {
            "persistent": true,
            "cache": false
        }
    }
}